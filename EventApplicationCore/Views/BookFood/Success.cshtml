@{
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}
<style>
    p > a {
        color: white;
    }
</style>
@if (ViewData["BookingFoodMessage"] != null)
{
    
    <p class="alert alert-success" id="successMessage" style="margin-top:110px;">@ViewData["BookingFoodMessage"]</p>
    <h3>Next Process Book Lighting</h3>
    <p class="btn btn-success" >
        @Html.ActionLink("Click Here to Book Lighting", "BookLight", "BookLight")
    </p>
}