@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model IEnumerable<EventApplicationCore.Model.BookingDetailTemp>
<script src="~/js/site.js"></script>
<script src="~/js/site.min.js"></script>

<link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" rel="stylesheet" />
<script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js "></script>

<div class="container">
    <br />
    <div style="width:90%; margin:0 auto;">

        <table id="myTable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
            <thead>
                <tr>
                 
                    <th>BookingNo</th>
                    <th>BookingDate</th>
                    <th>BookingApproval</th>
                    <th>BookingApprovalDate</th>
                    <th>Process</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.Count() > 0)
                {
                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>@Html.DisplayFor(m => item.BookingNo) </td>

                            <td>@Html.DisplayFor(m => item.BookingDate) </td>

                            <td>@Html.DisplayFor(m => item.BookingApproval) </td>

                            <td>@Html.DisplayFor(m => item.BookingApprovalDate) </td>

                            <td>
                            <td style=" width:150px;">
                                <div class="btn-group" role="group">
                                    <a class="btn btn-info" href="/ShowBookingDetails/BookingApproval?BookingNo=@item.BookingNo">Process</a>
                                     
                                </div>
                            </td> 

                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>



</div>
