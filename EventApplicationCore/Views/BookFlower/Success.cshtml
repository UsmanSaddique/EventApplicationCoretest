@{
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}
<style>
    p > a {
        color: white;
    }
</style>
@if (ViewData["BookingFlowerMessage"] != null)
{
    <p class="alert alert-success" id="successMessage" style="margin-top:110px;">@ViewData["BookingFlowerMessage"]</p>
    <h3>Process Completed Download Receipt</h3>
    <p class="btn btn-success">
        @Html.ActionLink("Show Receipt", "AllBookings", "ShowAllBooking")
    </p>
}